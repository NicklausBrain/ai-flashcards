@page "/HumanActivity"

@using blazor_app.Core;

@inject HttpClient Http
@inject NavigationManager NavigationManager

@code {
    private Core.FlashCard[]? humanActivity;

    protected override async Task OnInitializedAsync()
    {
        humanActivity = await Http.GetFromJsonAsync<Core.FlashCard[]>($"{NavigationManager.BaseUri}/data/human-activity.json");
    }
}

<FlashCardsList Cards="@humanActivity"></FlashCardsList>
