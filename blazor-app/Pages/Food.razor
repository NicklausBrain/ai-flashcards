@page "/Food"

@using blazor_app.Core;

@inject HttpClient Http
@inject NavigationManager NavigationManager

@code {
    private Core.FlashCard[]? food;

    protected override async Task OnInitializedAsync()
    {
        food = await Http.GetFromJsonAsync<Core.FlashCard[]>($"{NavigationManager.BaseUri}/data/food.json");
    }
}

<FlashCardsList Cards="@food"></FlashCardsList>
